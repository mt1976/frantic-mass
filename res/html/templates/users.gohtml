<!DOCTYPE html>
<html lang="en">
   {{template "head" .}}
    <body style="{{template "bodyStyle" .}}">

            {{template "breadcrumbs" .}}

            <article>
          
                <aside>
                    <nav>
                    <ul>
                    {{range .Users}}
                        {{if .Actions}}
                           {{range .Actions.Actions}}
                                <li><a href="{{.FormAction}}" role="button" style="width: 100%;" class="outline">{{.Icon}} {{.Name}}</a></li>
                            {{end}}
                        {{end}}
                    {{end}}
                    </ul>
                    </nav>
                </aside>
            </article>


        {{template "alerts" .}}

        {{template "actions" .Context.PageActions}}
   
    </body>
   {{template "footer" .}}
    {{template "scripts" .}}
</html>